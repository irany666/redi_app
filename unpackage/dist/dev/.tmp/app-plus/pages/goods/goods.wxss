
.e24-bg{
        background-color: #f9f9f9;
}
.e28-shape {
        background: rgb(9,187,7);
        width: 10px;
        height: 5px;
}
.e28-shape-lg {
        background: rgb(9,187,7);
        width: 10px;
        height: 5px;
}
.e26-title {
        position: relative;
}
.e26-title::before {
        content: '';
        display: block;
        position: absolute;
        top: 2px;
        left: -2px;
        width: 14px;
        height: 14px;
        border: 3px solid rgb(9,187,7);
        border-radius: 50%;
}
.e26-title-lg {
        position: relative;
}
.e26-title-lg::before {
        content: '';
        display: block;
        position: absolute;
        top: 4px;
        left: -2px;
        width:  16px;
        height: 16px;
        border: 3px solid rgb(9,187,7);
        border-radius: 50%;
}
.e25-title {
        position: relative;
}
.e25-title::before {
        content: '';
        display: block;
        position: absolute;
        top: 2px;
        left: 0;
        height: 14px;
        border-left: 5px solid rgb(9,187,7);
}
.e25-title-lg {
        position: relative;
}
.e25-title-lg::before {
        content: '';
        display: block;
        position: absolute;
        top: 4px;
        left: 0;
        height: 16px;
        border-left: 5px solid rgb(9,187,7);
}





